<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head.ejs') %>
</head>
<body>
    <header>
        <%- include('partials/header.ejs') %>
    </header>

    <main>
        <div class="jombotron">
            <h2>Variables</h2>
            <p>
                <%= tagline %>
            </p>

            <ul>
                <% pets.forEach((pet) => { %>
                    <li>
                        <strong>
                            <%= pet.name %>

                        </strong> representa a <%= pet.organization %>, nacio <%= pet.brith_year %>
                    </li> <% }); %>
            </ul>
        </div>
    </main>
    <footer>
        <%- include('partials/footer.ejs') %>
    </footer>
</body>
</html>